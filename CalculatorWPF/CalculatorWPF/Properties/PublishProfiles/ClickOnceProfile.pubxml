  <ItemGroup>
    <BootstrapperPackage Include="Microsoft.NetCore.DesktopRuntime.6.0.x86">
      <Install>true</Install>
      <ProductName>.NET Desktop Runtime 6.0.36 (x86)</ProductName>
    </BootstrapperPackage>
  </ItemGroup>
