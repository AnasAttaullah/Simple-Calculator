    <OpenBrowserOnPublish>False</OpenBrowserOnPublish>
    <Platform>Any CPU</Platform>
    <ProductName>Simple Calculator</ProductName>
    <PublishDir>bin\Release\net6.0-windows\win-x86\app.publish\</PublishDir>
